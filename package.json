{
  "private": true,
  "browserslist": [
    "last 2 versions",
    "not IE 11",
    "Firefox ESR"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production gulp build",
    "start": "cross-env NODE_ENV=development gulp start",
    "test": "npm run js:lint && npm run css:lint",
    "css:lint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "js:lint": "eslint source/js/**/*.js",
    "fix": "npm run js:fix && npm run css:fix",
    "js:fix": "eslint source/js/script.js --fix",
    "css:fix": "stylelint source/**/*.scss --fix"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.10",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.26.14",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "del": "^6.0.0",
    "eslint": "^7.21.0",
    "eslint-config-google": "^0.14.0",
    "gulp": "^4.0.2",
    "gulp-csso": "^4.0.1",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "webpack": "^4.42.0",
    "webpack-stream": "^5.2.1"
  }
}
